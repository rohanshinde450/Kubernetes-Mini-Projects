apiVersion: apps/v1
kind: ReplicationController
metadata:
   name: myrc2
spec:
  replicas: 3
  selector:
    matchLabels:
      env: testing
  template:
       metadata:
          labels:
              env: testing
       spec:
            containers:
                 - name: nginxcontainer
                   image: nginx
                   ports:
                     - containerPort: 80
